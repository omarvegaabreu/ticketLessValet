  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.20.1",
    "caniuse-lite": "^1.0.30001419",
    "chart.js": "^2.9.4",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.18.2",
